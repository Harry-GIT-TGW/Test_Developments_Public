{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.tgw.WarehouseTasksCharts",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZFIORI_ORDIM_C_SRV": {
				"uri": "/sap/opu/odata/tgw/ZFIORI_ORDIM_C_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/ZFIORI_ORDIM_C_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.tgw.WarehouseTasksCharts.view.App",
			"type": "XML"
			
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.tgw.WarehouseTasksCharts.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Request",
										"useBatch": true
				},
				"dataSource": "ZFIORI_ORDIM_C_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"viewPath": "com.tgw.WarehouseTasksCharts.view",
				"controlId": "idApp"
			},
			"routes": [{
				"name": "default",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["default"]
			}],
			"targets": {
				"default": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Main",
					"title": "Chart Demo",
					"viewId": "idMain",
					"viewLevel": 1,
					"controlAggregation": "pages"
				}
			}
		}
	}
}